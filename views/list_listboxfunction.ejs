<%-listbox %>

<script>
  var logic = document.frm.logic;
  var area = document.frm.area;
  var area1 = document.frm.area1;
  var station = document.frm.station;
  var view = document.frm.view;
  var logic = document.frm.logic;

  function viewchange()
  {
    var index = view.selectedIndex;
    var value = view.options[index].value;
    console.log(value);
    if(value=='')
    {
      area.style.display="none";
      area.selectedIndex=0;
      area1.style.display="none";
      area1.selectedIndex=0;
      station.style.display="none";
      station.selectedIndex=0;
      logic.selectedIndex=0;
      logic.style.display="none";
    }
    else if(value =='area')
    {
      area.style.display="inline";
      area1.style.display="inline";
      station.style.display="inline";
      area.selectedIndex=0;
      logic.selectedIndex=0;
      logic.style.display="none";
    }
    else if(value == 'logic')
    {
      logic.selectedIndex=0;
      logic.style.display="inline";
      area.style.display="none";
      area.selectedIndex=0;
      area1.style.display="none";
      area1.selectedIndex=0;
      station.style.display="none";
      station.selectedIndex=0;
    }
    else
    {
      area.style.display="none";
      area.selectedIndex=0;
      area1.style.display="none";
      area1.selectedIndex=0;
      station.style.display="none";
      station.selectedIndex=0;
      logic.selectedIndex=0;
      logic.style.display="none";
    }
  }

  function userview()
  {
    document.frm.action="/";
    document.frm.submit();
  }
</script>
