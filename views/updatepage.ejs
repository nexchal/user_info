<!DOCTYPE html>
<html>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, height=device-height, minimum-scale=1.0, maximum-scale=1.0, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome">
    <title>지능형 Web</title>
<head>
<script src="../assets/vendor/jquery/jquery.min.js"></script>
<script src="../assets/js/checkform.js"></script>
<script>
$(function () {
$('.select_one').change (function () {
    if ($('.select_one:checked').length == 0) {
      // 전부 체크안됨
        $('#real').removeClass('some_selected').attr('checked', false);
    } else if ($('.select_one:not(:checked)').length == 0) {
      // 일부만 체크됨
        $('#real').removeClass('some_selected').attr('checked', true);
    } else {
        // 전부 체크됨
        $('#real').addClass('some_selected').attr('checked', true);
    }
});
});
</script>
	<style>

  #title {text-decoration: none; color: white;}

    div.top {
        height:100px;
        width: 100%;
        display:table;
    }

    div.right {
        height: auto;
        width: 100%;
        float: right;
        position: relative;
    }
    div.bottom {
        bottom:0px;
        width: 100%;
        position:fixed;
    }
    li
    {
        padding: 5px;
    }

    .some_selected {
        opacity: 0.5;
        filter: alpha(opacity=50);
    }
  </style>
</head>
<body onload = TypeCheck()>


	  <div class="top"><%- frame_top %></div><br>
