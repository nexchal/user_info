            <label for="one">One</label>
        </li>
        <li>
            <input type="checkbox" id="two" name = "q" class="select_one">
            <label for="two">Two</label>
        </li>
        <li>
            <input type="checkbox" id="three" name = "q" class="select_one">
            <label for="three">Three</label>
        </li>
    </ul>

</body>

